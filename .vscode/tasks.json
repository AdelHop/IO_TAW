{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build-css",
        "type": "shell",
        "command": "sass",
        "args": [
          "scss:css"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "label": "lint",
        "type": "shell",
        "command": "eslint",
        "args": [
          "js/**/*.js"
        ],
        "problemMatcher": ["$eslint-stylish"]
      }
    ]
  }
  